<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.umn.wisuh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wisuh_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wisuh_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="id.ac.umn.wisuh.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.ac.umn.wisuh.NearbyActivity"></activity>
        <activity android:name="id.ac.umn.wisuh.HomeActivity"></activity>
        <activity android:name="id.ac.umn.wisuh.RegisterActivity"></activity>
        <activity android:name="id.ac.umn.wisuh.MainActivity"></activity>
    </application>

</manifest>